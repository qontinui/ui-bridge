{
  "name": "ui-bridge-native",
  "version": "0.1.1",
  "description": "UI Bridge Framework for React Native - Hooks and providers for UI observation and control in mobile apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./control": {
      "types": "./dist/control/index.d.ts",
      "import": "./dist/control/index.mjs",
      "require": "./dist/control/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    },
    "./debug": {
      "types": "./dist/debug/index.d.ts",
      "import": "./dist/debug/index.mjs",
      "require": "./dist/debug/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "react": "^19.1.0",
    "react-native": "^0.81.5",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qontinui/ui-bridge.git",
    "directory": "packages/ui-bridge-native"
  },
  "keywords": [
    "react-native",
    "ui",
    "automation",
    "testing",
    "ai",
    "mobile",
    "hooks"
  ]
}
